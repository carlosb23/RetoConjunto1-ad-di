<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="204.0" prefWidth="826.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.retoconjunto1addi.Controllers.VentanaDatosController">
   <center>
      <TableView fx:id="tabledato" prefHeight="159.0" prefWidth="536.0" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn fx:id="columid" prefWidth="94.0" text="ID" />
          <TableColumn fx:id="columcodigo" prefWidth="109.0" text="CODIGO" />
            <TableColumn fx:id="columcantidad" prefWidth="128.0" text="CANTIDAD" />
            <TableColumn fx:id="columproduct" prefWidth="492.0" text="PRODUCTO_ID" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </center>
   <top>
      <Button fx:id="volver" alignment="CENTER" mnemonicParsing="false" onAction="#btnvolverVP" prefHeight="25.0" prefWidth="74.0" text="Volver" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="700.0" top="10.0" />
         </BorderPane.margin>
      </Button>
   </top>
</BorderPane>
